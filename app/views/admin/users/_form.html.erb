<%= simple_form_for(@admin_user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :group %>
    <%= f.input :password %>
    <%= f.input :name %>
    <%= f.input :age %>
    <%= f.input :sex %>
    <%= f.input :phone %>
    <%= f.input :email %>
    <%= f.input :address %>
    <%= f.input :actived %>
    <%= f.input :aggrementno %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
