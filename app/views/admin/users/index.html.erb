<p id="notice"><%= notice %></p>

<h1>Admin Users</h1>

<table>
  <thead>
    <tr>
      <th>Group</th>
      <th>Password</th>
      <th>Name</th>
      <th>Age</th>
      <th>Sex</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Address</th>
      <th>Actived</th>
      <th>Aggrementno</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_users.each do |admin_user| %>
      <tr>
        <td><%= admin_user.group %></td>
        <td><%= admin_user.password %></td>
        <td><%= admin_user.name %></td>
        <td><%= admin_user.age %></td>
        <td><%= admin_user.sex %></td>
        <td><%= admin_user.phone %></td>
        <td><%= admin_user.email %></td>
        <td><%= admin_user.address %></td>
        <td><%= admin_user.actived %></td>
        <td><%= admin_user.aggrementno %></td>
        <td><%= admin_user.description %></td>
        <td><%= link_to 'Show', admin_user %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(admin_user) %></td>
        <td><%= link_to 'Destroy', admin_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin User', new_admin_user_path %>
