<!-- BEGIN FORM-->
<%= simple_form_for :search, url: admin_renters_path, :method => :get, :html => {class: 'form-horizontal'} do |f| %>
    <div class="form-body">
      <div class="form-group form-md-line-input">
        <label class="col-md-2 control-label"><%= t(:renter_code)%></label>
        <div class="col-md-4">
          <%= f.input_field :code, class: 'bg-maxlength form-control', placeholder: t(:renter_code_placehoder), maxlength: 255 %>
          <div class="form-control-focus"></div>
        </div>
        <!--<div class="col-md-6">-->
        <!--<input type="text" class="form-control" id="form_control_1" placeholder="Nhập tiêu chí tìm kiếm">-->
        <!--<div class="form-control-focus"></div>-->
        <!--</div>-->
      </div>
    </div>

    <div class="form-actions margin-top-10">
      <div class="row">
        <div class="col-md-12 text-right">
          <%= f.button :button, class: 'btn green' do %>
              <i class="fa fa-search" aria-hidden="true"></i><%= t(:search)%>
          <% end %>
        </div>
      </div>
    </div>
<% end %>
<!-- END FORM-->